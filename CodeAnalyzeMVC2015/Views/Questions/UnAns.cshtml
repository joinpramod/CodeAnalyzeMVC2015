@{
    ViewBag.Title = "UnAns";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table style="width: 100%; font-size: 16px">
    @foreach (CodeAnalyzeMVC2015.Models.QuestionModel questionModel in Model)
    {
        <tr>
            <td>
                <img id="imgArrow" src="~/Images/questions.png" style="width:20px;height:20px;"  />
            </td>
            <td>
                @Html.ActionLink(questionModel.QuestionTitle, "Soln", "Questions", new { id = questionModel.QuestionID, title = questionModel.QuestionTitle.ToString().Replace(" ", "-") }, null)
            </td>
        </tr>
    }
</table>
